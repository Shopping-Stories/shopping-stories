{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env FORCE_COLOR=true next dev | pino-pretty --colorize",
    "build": "next build",
    "start": "NODE_ENV=production next start",
    "lint": "next lint",
    "format": "prettier --write \"server/**/*.ts\" \"client/**/*.ts\" \"tests/**/*.ts\" \"pages/**/*.ts\" \"pages/**/*.tsx\"",
    "test": "cross-env NODE_ENV=test jest --passWithNoTests --detectOpenHandles --runInBand",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:cov": "cross-env NODE_ENV=test jest --coverage",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^1.2.16",
    "@typegoose/typegoose": "^8.2.0",
    "@types/graphql-fields": "^1.3.4",
    "@types/jwk-to-pem": "^2.0.1",
    "@types/uuid": "^8.3.1",
    "@urql/exchange-auth": "^0.1.6",
    "@urql/exchange-graphcache": "^4.3.5",
    "amazon-cognito-identity-js": "^5.1.1",
    "antd": "^4.16.13",
    "apollo-server-core": "^3.3.0",
    "apollo-server-micro": "^3.3.0",
    "aws-amplify": "^4.2.7",
    "aws-sdk": "^2.991.0",
    "axios": "^0.21.4",
    "babel-plugin-import": "^1.13.3",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "export-from-json": "^1.4.0",
    "graphql": "^15.5.3",
    "graphql-fields": "^2.0.3",
    "graphql-tag": "^2.12.5",
    "graphql-type-json": "^0.3.2",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.5",
    "micro": "^9.3.4",
    "mongodb": "^4.1.1",
    "mongodb-memory-server": "^7.4.1",
    "mongoose": "^5.13.9",
    "next": "11.1.2",
    "next-auth": "^4.0.0-beta.2",
    "next-plugin-antd-less": "^1.4.3",
    "next-urql": "^3.2.1",
    "node-mocks-http": "^1.10.1",
    "pino": "^7.0.0-rc.3",
    "pino-logflare": "^0.3.12",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "^17.0.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.3.0",
    "type-graphql": "^1.1.1",
    "urql": "^2.0.5",
    "uuid": "^8.3.2",
    "xlsx": "^0.17.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.15.4",
    "@babel/plugin-proposal-private-methods": "^7.14.5",
    "@babel/plugin-proposal-private-property-in-object": "^7.15.4",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/pino": "^6.3.11",
    "@types/react": "17.0.19",
    "@types/supertest": "^2.0.11",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "cross-env": "^7.0.3",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "jest": "^27.1.0",
    "pino-pretty": "^6.0.0",
    "prettier": "^2.3.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "4.4.2"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.next/"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
