{
    "name": "client",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "cross-env FORCE_COLOR=true next dev | pino-pretty --colorize",
        "build": "cross-env next build",
        "start": "cross-env NODE_ENV=production next start",
        "lint": "cross-env next lint",
        "format": "cross-env prettier --write \"server/**/*.ts\" \"client/**/*.tsx\" \"client/**/*.ts\" \"tests/**/*.ts\" \"pages/**/*.ts\" \"pages/**/*.tsx\"",
        "test": "cross-env NODE_ENV=test jest --passWithNoTests --detectOpenHandles --runInBand",
        "test:watch": "cross-env NODE_ENV=test jest --watch",
        "test:cov": "cross-env NODE_ENV=test jest --coverage",
        "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "typecheck": "tsc",
        "codegen": "graphql-codegen --config codegen.ts"
    },
    "dependencies": {
        "@emotion/cache": "^11.4.0",
        "@emotion/react": "^11.4.1",
        "@emotion/server": "^11.4.0",
        "@emotion/styled": "^11.3.0",
        "@mui/icons-material": "^5.0.4",
        "@mui/lab": "^5.0.0-alpha.51",
        "@mui/material": "^5.0.0-rc.1",
        "@mui/styles": "^5.0.1",
        "@mui/system": "^5.0.4",
        "@mui/x-data-grid": "^5.17.26",
        "@tanstack/react-query": "^4.20.4",
        "@typegoose/typegoose": "^8.2.0",
        "@types/d3-force": "^3.0.4",
        "@types/graphql-fields": "^1.3.4",
        "@types/jwk-to-pem": "^2.0.1",
        "@types/lodash": "^4.14.177",
        "@types/uuid": "^8.3.1",
        "@urql/exchange-auth": "^0.1.6",
        "@urql/exchange-graphcache": "^4.3.5",
        "apollo-server-core": "^3.3.0",
        "apollo-server-micro": "^3.3.0",
        "aws-amplify": "^4.2.7",
        "aws-sdk": "^2.991.0",
        "axios": "^0.21.4",
        "babel-plugin-import": "^1.13.3",
        "class-transformer": "^0.4.0",
        "class-validator": "^0.13.1",
        "clsx": "^1.1.1",
        "d3-force": "^3.0.0",
        "export-from-json": "^1.4.0",
        "formik": "^2.2.9",
        "graphql": "^15.5.3",
        "graphql-fields": "^2.0.3",
        "graphql-tag": "^2.12.6",
        "graphql-type-json": "^0.3.2",
        "jsonwebtoken": "^8.5.1",
        "jwk-to-pem": "^2.0.5",
        "lodash": "^4.17.21",
        "micro": "^9.3.4",
        "mongodb": "^4.1.1",
        "mongodb-memory-server": "^7.4.1",
        "mongoose": "^5.13.9",
        "next": "11.1.2",
        "next-images": "^1.8.2",
        "next-urql": "^3.2.1",
        "node-mocks-http": "^1.10.1",
        "pino": "^7.0.0-rc.3",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-force-graph-2d": "^1.23.17",
        "react-is": "^17.0.2",
        "react-modal": "^3.16.1",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.3.0",
        "sharp": "^0.29.3",
        "type-graphql": "^1.1.1",
        "urql": "^2.0.5",
        "uuid": "^8.3.2",
        "xlsx": "^0.17.1",
        "yup": "^0.32.11"
    },
    "devDependencies": {
        "@babel/core": "^7.15.5",
        "@babel/plugin-proposal-class-properties": "^7.14.5",
        "@babel/plugin-proposal-decorators": "^7.15.4",
        "@babel/plugin-proposal-private-methods": "^7.14.5",
        "@babel/plugin-proposal-private-property-in-object": "^7.15.4",
        "@graphql-codegen/cli": "2.13.7",
        "@graphql-codegen/client-preset": "1.0.7",
        "@graphql-codegen/typescript": "^2.7.4",
        "@graphql-codegen/typescript-operations": "^2.5.4",
        "@graphql-codegen/typescript-urql": "^3.7.1",
        "@types/jest": "^27.0.1",
        "@types/jsonwebtoken": "^8.5.5",
        "@types/pino": "^6.3.11",
        "@types/react": "17.0.19",
        "@types/react-modal": "^3.13.1",
        "@types/supertest": "^2.0.11",
        "babel-plugin-transform-typescript-metadata": "^0.3.2",
        "cross-env": "^7.0.3",
        "eslint": "7.32.0",
        "eslint-config-next": "11.1.2",
        "jest": "^27.1.0",
        "pino-pretty": "^6.0.0",
        "prettier": "^2.3.2",
        "supertest": "^6.1.6",
        "ts-jest": "^27.0.5",
        "ts-node": "^10.2.1",
        "typescript": "4.4.2"
    },
    "jest": {
        "testPathIgnorePatterns": [
            "/node_modules/",
            "/.next/"
        ],
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
